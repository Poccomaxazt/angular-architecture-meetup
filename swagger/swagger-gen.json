{
  "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "./swagger/swagger.json",
  "output": "./src/app/api",
  "config": "./swagger/swagger-gen.json",
  "ignoreUnusedModels": false,
  "minParamsForContainer": 2,
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true,
  "apiModule": true,
  "templates": "./swagger/templates"
}
